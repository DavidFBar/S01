package main
import (
	"fmt"
)
func fibonacci(n int) int{
	var a,b int = 0,1
	var c int
	if n <= 0{
		fmt.Println("O numero deve ser maior que zero")
	}else if n == 1 {
		fmt.Println(a)
	}else{
		fmt.Print(a, " ", b)
		for i := 2; i < n; i++ {
			c = a + b
			fmt.Print(" ", c)
			a, b = b, c
		}
	}
	return 0
}
func main() {
	fmt.Println("Quantos numeros da sequencia de fibonacci voce quer?")
	var n int
	fmt.Scan(&n)
	fibonacci(n)
}
