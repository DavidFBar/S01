package main
import (
	"fmt"
)
func classificar_numero(num int) string {
	if num > 0 {
		return "Positivo"
	}else if num < 0 {
		return "Negativo"
	}else{
		return "Zero"
}
}
func main() {
	 fmt.Println("Insira um numero qualquer:")
	 var num int
	 fmt.Scan(&num)
	 var res string = classificar_numero(num)
	 fmt.Println(res)
}
